{
  "name": "@foundriesio/redis-cache",
  "private": false,
  "version": "2.0.6",
  "description": "A redis-based cache",
  "type": "module",
  "main": "./dist/main.cjs",
  "module": "./src/index.js",
  "exports": {
    ".": {
      "require": "./dist/main.cjs",
      "default": "./src/index.js"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "LICENSE.md",
    "package-lock.json",
    "dist",
    "src"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "prepare": "npm run build",
    "build": "NODE_ENV=production webpack --config webpack/webpack.config.js --progress",
    "lint": "prettier *.md *.js ./src/*.js ./src/**/*.js  --write",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "src/**/*.+(js)": [
      "eslint --config ./src/.eslintrc.json --fix",
      "prettier --write"
    ],
    "src/**/*.+(md|css|scss|yaml)": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "backend",
    "cache",
    "foundriesio",
    "redis"
  ],
  "author": "Milo Casagrande <milo@foundries.io>",
  "license": "Apache-2.0",
  "peerDependencies": {
    "@foundriesio/redis-client": ">=2"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.23.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/foundriesio/fio-redis-cache.git"
  },
  "bugs": {
    "url": "https://github.com/foundriesio/fio-redis-cache/issues"
  },
  "homepage": "https://github.com/foundriesio/fio-redis-cache#readme"
}
